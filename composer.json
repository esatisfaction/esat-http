{
    "name": "esat/http",
    "description": "Http Package for Esat",
    "license": "MIT",
    "support": {
        "source": "https://bitbucket.org/esatisfaction/http",
        "issues": "https://bitbucket.org/esatisfaction/http/issues"
    },
    "authors": [
        {
            "name": "e-satisfaction Developers",
            "email": "tech@e-satisfaction.com",
            "homepage": "https://developers.e-satisfaction.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:esatisfaction/esat-model.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:esatisfaction/esat-helpers-php.git"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "symfony/cache": "^4.2",
        "symfony/http-foundation": "^3.3",
        "guzzlehttp/guzzle": "^6.0",
        "esat/helpers": "^1.0",
        "esat/model": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Esat\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esat\\Http\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
