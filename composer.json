{
    "name": "esat/http",
    "description": "Http Library for Esat",
    "license": "MIT",
    "support": {
        "source": "https://bitbucket.org/esatisfaction/http",
        "issues": "https://bitbucket.org/esatisfaction/http/issues"
    },
    "authors": [
        {
            "name": "e-satisfaction Developers",
            "email": "tech@e-satisfaction.com",
            "homepage": "https://tech.e-satisfaction.com"
        }
